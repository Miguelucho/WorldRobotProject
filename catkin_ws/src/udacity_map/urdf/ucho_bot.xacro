<?xml version='1.0'?>

<robot name="ucho_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find udacity_map)/urdf/ucho_bot.gazebo" />

  <link name="robot_footprint"></link>

<!--Chassis-->
  <link name="chassis">
    
    <!--pose>0 0 0 0 0 0</pose-->

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
				<!--Piece1-->
    <collision name='piece1_coll'>
      <origin xyz="0.1 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </collision>

    <visual name='piece1_visu'>
      <origin xyz="0.1 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </visual>
	
				<!--Piece2-->
    <collision name='piece2_coll'>
      <origin xyz="0.05 0 0.1" rpy=" 1.5708 0 0"/> 
      <geometry>
        <box size=".3 .2 .1"/>
      </geometry>
    </collision>

    <visual name='piece2_visu'>
      <origin xyz="0.05 0 0.1" rpy=" 1.5708 0 0"/>
      <geometry>
        <box size=".3 .2 .1"/>
      </geometry>
    </visual>
	
				<!--Piece3-->
    <collision name='piece3_coll'>
      <origin xyz="0.05 -0.049 0.116" rpy=" -0.32175 0 0"/> 
      <geometry>
        <box size=".3 .0560 .1581"/>
      </geometry>
    </collision>

    <visual name='piece3_visu'>
      <origin xyz="0.05 -0.049 0.116" rpy=" -0.32175 0 0"/>
      <geometry>
        <box size=".3 .0560 .1581"/>
      </geometry>
    </visual>
	
				<!--Piece4-->
    <collision name='piece4_coll'>
      <origin xyz="0.05 0.049 0.116" rpy=" 0.32175 0 0"/> 
      <geometry>
        <box size=".3 .0560 .1581"/>
      </geometry>
    </collision>

    <visual name='piece4_visu'>
      <origin xyz="0.05 0.049 0.116" rpy=" 0.32175 0 0"/>
      <geometry>
        <box size=".3 .0560 .1581"/>
      </geometry>
    </visual>

				<!--Piece5-->
    <collision name='piece5_coll'>
      <origin xyz="0.20 0 0.175" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".1 .1 .5"/>
      </geometry>
    </collision>

    <visual name='piece5_visu'>
      <origin xyz="0.20 0 0.175" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .1 .05"/>
      </geometry>
    </visual>

                                <!--BallBack-->
    <collision name='back_caster_collision'>
      <origin xyz="0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.049"/>
      </geometry>
    </collision>

    <visual name='back_caster_visual'>
      <origin xyz="0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

                               <!--BallFront-->
    <collision name='front_caster_collision'>
      <origin xyz="0.2 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.049"/>
      </geometry>
    </collision>

    <visual name='front_caster_visual'>
      <origin xyz="0.2 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

                               <!--BarBack-->    
    <collision name='back_barra_collision'>
      <origin xyz="-0.1 0 -0.075" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .1 .05"/>
      </geometry>
    </collision>

    <visual name='back_barra_visual'>
      <origin xyz="-0.1 0 -0.075" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .1 .05"/>
      </geometry>
    </visual>

  </link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>
    
<!--Wheels-->  
  <link name="left_wheel">
  
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>
    
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>
    
  </link>

  <joint type="continuous" name="left_wheel_hinge" >
    <origin xyz="0 0.15 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="10000" velocity="1000" />
    <joint_properties damping="1.0" friction="1.0" />
  </joint>

  <link name="right_wheel">
    
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>
    
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint type="continuous" name="right_wheel_hinge" >
    <origin xyz="0 -0.15 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="10000" velocity="1000" />
    <joint_properties damping="1.0" friction="1.0" />
  </joint>

<!--Camera-->
  <link name="camera">
  
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
    
    <collision>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.3 0.01" />
      </geometry>
    </collision>
    
    <visual>
      <origin xyz="0.025 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename="package://udacity_map/meshes/kinect.dae" />
        <!--box size="0.05 0.2 0.05" /-->
      </geometry>
    </visual>
    
  </link>
  
  <joint type="fixed" name="camera_link" >
    <origin xyz="0.25 0 0.1" rpy="0 0 0" />
    <child link="camera" />
    <parent link="chassis" />
  </joint>

<!--Laser-->
  <link name="hokuyo">
    
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
    
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
    </collision>
    
    <visual>
      <origin xyz="0 0 -0.025" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://udacity_map/meshes/hokuyo.dae" />
        <!-- <box size="0.1 0.1 0.1" /> -->
      </geometry>
    </visual>
    
  </link>

<!--Joint-->
  <joint name="camera_joint" type="fixed">
      <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
      <parent link="camera"/>
      <child link="camera_rgbd_frame"/>
  </joint>
  
<link name="camera_rgbd_frame"/>


  <joint type="fixed" name="hokuyo_joint" >
    <origin xyz="0.20 0 0.25" />
    <child link="hokuyo" />
    <parent link="chassis" />
  </joint>

</robot>
